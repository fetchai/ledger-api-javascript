{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true,
    "node": true,
    "jest/globals": true
  },
  "plugins": [
    "jest"
  ],
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      4
    ],
    "no-prototype-builtins": "off",
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "never"
    ],
    "jest/no-disabled-tests": [
      "off"
    ],
    "jest/no-focused-tests": [
      "error"
    ],
    "jest/no-identical-title": [
      "error"
    ],
    "jest/prefer-to-have-length": [
      "warn"
    ],
    "jest/valid-expect": [
      "error"
    ]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  }
}
